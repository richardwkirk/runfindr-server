{
  "name": "runfindr-server",
  "version": "1.0.0",
  "description": "runfindr server",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "build-watch": "tsc --watch",
    "start": "node dist/index.js",
    "dev": "nodemon dist/index.js --watch src"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richardwkirk/runfindr-server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/richardwkirk/runfindr-server/issues"
  },
  "homepage": "https://github.com/richardwkirk/runfindr-server#readme",
  "dependencies": {
    "@types/node": "^12.6.8",
    "express": "^4.17.1",
    "htmlparser2": "^3.10.1",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/express": "^4.17.0"
  }
}
